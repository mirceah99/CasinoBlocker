{
  "name": "Casino blocker",
  "description": "Chrome extension that helps you overpass your casino addiction!",
  "version": "0.1",
  "permissions": ["tabs"],
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "logo.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["bannedSites.json", "replace.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
